{
  "compilerOptions": {
    "incremental": true,
    "target": "es2022",
    "module": "es2022",
    "declaration": true,
    "rootDir": "./src",
    "moduleResolution": "bundler",
    "outDir": "dist",
    "baseUrl": "./src",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true
  }
}
