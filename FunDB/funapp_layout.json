{
  "schemas": {
    "funapp": {
      "entities": {
        "Settings": {
          "columnFields": {
            "Name": { "fieldType": "string" },
            "Value": { "fieldType": "string" }
          },
          "uniqueConstraints": {
            "UniqueName": { "columns": [ "Name" ] }
          },
          "checkConstraints": {
            "NonEmptyName": { "expression": "\"Name\" <> ''" }
          },
          "mainField": "Name"
        },

        "MainMenuCategories": {
          "columnFields": {
            "Name": { "fieldType": "string" },
            "OrdinalPosition": { "fieldType": "int" }
          },
          "uniqueConstraints": {
            "UniqueName": { "columns": [ "Name" ] }
          },
          "checkConstraints": {
            "NonEmptyName": { "expression": "\"Name\" <> ''" }
          },
          "mainField": "Name"
        },

        "MainMenuButtons": {
          "columnFields": {
            "CategoryId": { "fieldType": "reference(\"funapp\".\"MainMenuCategories\")" },
            "UserViewId": { "fieldType": "reference(\"public\".\"UserViews\")" },
            "OrdinalPosition": { "fieldType": "int" }
          },
          "computedFields": {
            "Name": { "expression": "\"CategoryId\" || '.' || \"UserViewId\"" }
          },
          "uniqueConstraints": {
            "UniqueButton": { "columns": [ "CategoryId", "UserViewId" ] }
          },
          "mainField": "Name"
        },

        "UserViewTranslations": {
          "columnFields": {
            "UserViewId": { "fieldType": "reference(\"public\".\"UserViews\")" },
            "Language": { "fieldType": "string" },
            "Translation": { "fieldType": "string" }
          },
          "computedFields": {
            "Name": { "expression": "\"Language\" || '.' || \"UserViewId\"->\"__main\"" }
          },
          "uniqueConstraints": {
            "UniqueTranslation": { "columns": [ "UserViewId", "Language" ] }
          },
          "checkConstraints": {
            "NonEmptyLanguage": { "expression": "\"Language\" <> ''" }
          },
          "mainField": "Name"
        },

        "ColumnFieldTranslations": {
          "columnFields": {
            "ColumnFieldId": { "fieldType": "reference(\"public\".\"ColumnFields\")" },
            "Language": { "fieldType": "string" },
            "Translation": { "fieldType": "string" }
          },
          "computedFields": {
            "Name": { "expression": "\"Language\" || '.' || \"ColumnFieldId\"->\"__main\"" }
          },
          "uniqueConstraints": {
            "UniqueTranslation": { "columns": [ "ColumnFieldId", "Language" ] }
          },
          "checkConstraints": {
            "NonEmptyLanguage": { "expression": "\"Language\" <> ''" }
          },
          "mainField": "Name"
        },

        "ComputedFieldTranslations": {
          "columnFields": {
            "ComputedFieldId": { "fieldType": "reference(\"public\".\"ComputedFields\")" },
            "Language": { "fieldType": "string" },
            "Translation": { "fieldType": "string" }
          },
          "computedFields": {
            "Name": { "expression": "\"Language\" || '.' || \"ComputedFieldId\"->\"__main\"" }
          },
          "uniqueConstraints": {
            "UniqueTranslation": { "columns": [ "ComputedFieldId", "Language" ] }
          },
          "checkConstraints": {
            "NonEmptyLanguage": { "expression": "\"Language\" <> ''" }
          },
          "mainField": "Name"
        }

      }
    }
  }
}
